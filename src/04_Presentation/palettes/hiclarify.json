{
  "color": {
    "surface": "#FFFFFF",
    "surfaceVariant": "#E8EAED",
    "surfaceHero": "#E8F5E9",
    "primary": "#2ECC71",
    "primaryVariant": "#27AE60",
    "onPrimary": "#FFFFFF",
    "onSurface": "#1A1D21",
    "outline": "#E0E0E2",
    "outlineVariant": "#C9CCD1",
    "outlineStrong": "#8E9199",
    "secondary": "#F97316",
    "error": "#C5221F",
    "errorContainer": "#FDDAD6",
    "onErrorContainer": "#410002"
  },

  "surface": {
    "app": "#F8FAFC",
    "section": "#FAFAFA",
    "card": "#FFFFFF",
    "elevated": "#FFFFFF",
    "base": "#FFFFFF",
    "variant": "#E8EAED",
    "hero": "#E8F5E9"
  },
  
  
  "radius": {
    "none": 0,
    "sm": 8,
    "md": 12,
    "lg": 18,
    "xl": 24,
    "pill": 9999
  },
  
  
  "padding": {
    "none": 0,
    "xs": 6,
    "sm": 12,
    "md": 20,
    "lg": 32,
    "xl": 48,
    "2xl": 72,
    "3xl": 112
  },


  "gap": {
    "xs": 6,
    "sm": 12,
    "md": 20,
    "lg": 32,
    "xl": 48,
    "2xl": 72,
    "3xl": 112
  },

  "spacing": {
    "sectionPadding": "padding.lg",
    "cardPadding": "padding.md",
    "inlinePadding": "padding.sm",
    "stackGap": "gap.md",
    "inlineGap": "gap.sm",
    "compactGap": "gap.xs"
  },
  
  
  "textSize": {
    "xs": 12,
    "sm": 14,
    "md": 16,
    "lg": 18,
    "xl": 20,
    "display": 64,
    "headline": 44,
    "title": 30,
    "bodyLg": 18,
    "body": 16,
    "caption": 14
  },


  "textWeight": {
    "regular": 400,
    "medium": 500,
    "semibold": 600,
    "bold": 700
  },
  
  
  "lineHeight": {
    "display": 1.05,
    "headline": 1.15,
    "title": 1.25,
    "tight": 1.25,
    "normal": 1.5,
    "relaxed": 1.65
  },
  
  
  "letterSpacing": {
    "tight": -0.2,
    "normal": 0,
    "loose": 0.4
  },
  
  
  "shadow": {
    "none": "none",
    "sm": "0px 1px 2px rgba(0,0,0,0.06)",
    "md": "0px 2px 4px rgba(0,0,0,0.08)",
    "lg": "0px 4px 8px rgba(0,0,0,0.12)"
  },
  
  
  "opacity": {
    "full": 1.0,
    "dim": 0.9,
    "low": 0.8
  },
  
  
  "borderWidth": {
    "none": 0,
    "sm": 1,
    "md": 2
  },
  
  
  "size": {
    "xs": 20,
    "sm": 32,
    "md": 48,
    "lg": 64
  },

  "textRole": {
    "display": { "size": "textSize.display", "weight": "textWeight.bold", "lineHeight": "lineHeight.display", "letterSpacing": "letterSpacing.tight" },
    "headline": { "size": "textSize.headline", "weight": "textWeight.bold", "lineHeight": "lineHeight.headline", "letterSpacing": "letterSpacing.tight" },
    "title": { "size": "textSize.title", "weight": "textWeight.semibold", "lineHeight": "lineHeight.title" },
    "subtitle": { "size": "textSize.lg", "weight": "textWeight.medium", "lineHeight": "lineHeight.normal", "color": "color.secondary" },
    "bodyLg": { "size": "textSize.bodyLg", "weight": "textWeight.regular", "lineHeight": "lineHeight.relaxed" },
    "body": { "size": "textSize.body", "weight": "textWeight.regular", "lineHeight": "lineHeight.relaxed" },
    "label": { "size": "textSize.sm", "weight": "textWeight.semibold", "lineHeight": "lineHeight.normal", "letterSpacing": "letterSpacing.loose" },
    "caption": { "size": "textSize.caption", "weight": "textWeight.regular", "lineHeight": "lineHeight.normal", "color": "color.secondary" }
  },

  "surfaceTier": {
    "base": { "background": "color.surface", "shadow": "shadow.none" },
    "raised": { "background": "color.surface", "shadow": "shadow.sm" },
    "overlay": { "background": "color.surface", "shadow": "shadow.md" },
    "floating": { "background": "color.surface", "shadow": "shadow.lg" }
  },

  "elevation": {
    "0": "none",
    "1": "0 1px 2px rgba(0,0,0,0.04), 0 1px 3px rgba(0,0,0,0.03)",
    "2": "0 4px 8px rgba(0,0,0,0.06), 0 2px 4px rgba(0,0,0,0.04)",
    "3": "0 12px 24px rgba(0,0,0,0.08), 0 8px 16px rgba(0,0,0,0.06)",
    "4": "0 20px 40px rgba(0,0,0,0.10), 0 12px 24px rgba(0,0,0,0.08)",
    "none": "none",
    "low": "0 1px 2px rgba(0,0,0,0.04), 0 1px 3px rgba(0,0,0,0.03)",
    "mid": "0 4px 8px rgba(0,0,0,0.06), 0 2px 4px rgba(0,0,0,0.04)",
    "strong": "0 12px 24px rgba(0,0,0,0.08), 0 8px 16px rgba(0,0,0,0.06)",
    "float": "0 20px 40px rgba(0,0,0,0.10), 0 12px 24px rgba(0,0,0,0.08)"
  },

  "prominence": {
    "primary": { "background": "color.primary", "color": "color.onPrimary" },
    "secondary": { "background": "color.surfaceVariant", "color": "color.onSurface" },
    "tertiary": { "background": "color.surface", "color": "color.onSurface" }
  },

  "fontFamily": {
    "base": "Inter, 'Segoe UI', system-ui, -apple-system, sans-serif",
    "sans": "Inter, 'Segoe UI', system-ui, -apple-system, sans-serif",
    "heading": "Inter, 'Segoe UI', system-ui, -apple-system, sans-serif",
    "mono": "'JetBrains Mono', 'Fira Code', ui-monospace, monospace"
  },

  "transition": {
    "fast": "150ms ease",
    "base": "200ms ease",
    "slow": "300ms ease",
    "none": "none"
  },

  "transform": {
    "hoverLift": "translateY(-1px)"
  },

  "interaction": {
    "hover": { "opacity": 0.92, "scale": 1.02, "lift": "translateY(-2px)" },
    "press": { "opacity": 0.85, "scale": 0.98 },
    "disabled": { "opacity": 0.38 }
  }
}
