    "outlineStrong": "#8B7355",
    "secondary": "#E65100",
    "error": "#8B0000",
    "errorContainer": "#F8D7DA",
    "onErrorContainer": "#3B0008"
  },
  "text": { "primary": "#1A1410", "muted": "#8B7355", "strong": "#1A1410" },
  "accent": { "primary": "#B71C1C", "soft": "#EDE4D8" },
  "border": { "soft": "#C4B5A0", "strong": "#8B7355" },
  "radius": {
    "none": 0,
    "sm": 8,
    "md": 12,
    "lg": 18,
    "xl": 24
  },


  "padding": {
    "none": 0,
    "xs": 6,
    "sm": 12,
    "md": 20,
    "lg": 32,
    "xl": 48,
    "2xl": 72,
    "3xl": 112
  },


  "gap": {
    "xs": 6,
    "sm": 12,
    "md": 20,
    "lg": 32,
    "xl": 48,
    "2xl": 72,
    "3xl": 112
