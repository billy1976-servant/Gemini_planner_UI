{
  "page": { "background": "#FFF9F3", "foreground": "#1A1410" },
  "color": {
    "surface": "#FFFBF5",
    "surfaceVariant": "#EDE4D8",
    "primary": "#B71C1C",
    "primaryVariant": "#7F0000",
    "onPrimary": "#FFFFFF",
    "onSurface": "#1A1410",
    "outline": "#C4B5A0",
