      },
      "splitLayout": {
        "display": "grid",
        "gridTemplateColumns": "1fr 1fr",
        "alignItems": "center"
      },
      "contentColumn": {
        "display": "flex",
        "flexDirection": "column",
        "gap": "0",
        "alignItems": "flex-start",
        "minWidth": "0"
      },
