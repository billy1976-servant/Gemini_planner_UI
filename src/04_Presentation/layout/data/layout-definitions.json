{
  "pageLayouts": {
    "hero-centered": {"containerWidth": "wide"},
    "hero-split": {"containerWidth": "full", "split": {"type": "split", "mediaSlot": "right"}},
    "hero-split-image-right": {"containerWidth": "full", "split": {"type": "split", "mediaSlot": "right"}},
    "hero-split-image-left": {"containerWidth": "full", "split": {"type": "split", "mediaSlot": "left"}},
    "hero-full-bleed-image": {"containerWidth": "full", "backgroundVariant": "default"},
    "content-narrow": {"containerWidth": "narrow"},
    "content-stack": {"containerWidth": "contained"},
    "image-left-text-right": {"containerWidth": "contained"},
    "features-grid-3": {"containerWidth": "contained"},
    "feature-grid-3": {"containerWidth": "contained"},
    "testimonial-band": {"containerWidth": "contained"},
    "cta-centered": {"containerWidth": "contained"},
    "test-extensible": {"containerWidth": "contained"}
  },
  "templates": {
    "startup-template": {"hero": "hero-split", "features": "features-grid-3", "content": "content-stack"}
  },
  "componentLayouts": {
    "hero-centered": {"type": "column", "preset": null, "params": {"gap": "var(--spacing-8)", "align": "center", "justify": "center", "padding": "var(--spacing-14) var(--spacing-6)"}},
    "hero-split": {"type": "row", "preset": null, "params": {"gap": "var(--spacing-12)", "align": "center", "justify": "space-between", "padding": "var(--spacing-20) var(--spacing-8)", "minHeight": "min(60vh, 560px)"}},
    "hero-split-image-right": {"type": "row", "preset": null, "params": {"gap": "var(--spacing-12)", "align": "center", "justify": "space-between", "padding": "var(--spacing-20) var(--spacing-8)", "minHeight": "min(60vh, 560px)"}},
    "hero-split-image-left": {"type": "row", "preset": null, "params": {"gap": "var(--spacing-12)", "align": "center", "justify": "space-between", "padding": "var(--spacing-20) var(--spacing-8)", "minHeight": "min(60vh, 560px)"}},
    "hero-full-bleed-image": {"type": "column", "preset": null, "params": {"gap": "var(--spacing-6)", "align": "center", "justify": "center", "padding": "var(--spacing-16) var(--spacing-6)", "minHeight": "420px", "maxHeight": "70vh", "display": "flex", "alignItems": "center", "justifyContent": "center"}},
    "content-narrow": {"type": "column", "preset": null, "params": {"gap": "var(--spacing-6)", "align": "stretch", "padding": "var(--spacing-8) 0"}},
    "content-stack": {"type": "column", "preset": null, "params": {"gap": "var(--spacing-6)", "align": "stretch", "padding": "var(--spacing-8) 0"}},
    "image-left-text-right": {"type": "row", "preset": null, "params": {"gap": "var(--spacing-8)", "align": "center", "justify": "flex-start", "padding": "var(--spacing-8) 0"}},
    "features-grid-3": {"type": "grid", "preset": null, "params": {"columns": 3, "gap": "var(--spacing-6)", "padding": "var(--spacing-8) 0"}},
    "feature-grid-3": {"type": "grid", "preset": null, "params": {"columns": 3, "gap": "var(--spacing-6)", "padding": "var(--spacing-8) 0"}},
    "testimonial-band": {"type": "row", "preset": null, "params": {"gap": "var(--spacing-8)", "align": "stretch", "justify": "center", "padding": "var(--spacing-10) var(--spacing-6)"}},
    "cta-centered": {"type": "column", "preset": null, "params": {"gap": "var(--spacing-6)", "align": "center", "justify": "center", "padding": "var(--spacing-10) var(--spacing-6)"}}
  }
}
