{
    "color": {
      "surface": "#FFFFFF",
      "surfaceVariant": "#F2F2F2",
      "primary": "#0D47A1",
      "primaryVariant": "#08306B",
      "onPrimary": "#FFFFFF",
      "onSurface": "#000000",
      "outline": "#9E9E9E",
      "secondary": "#424242",
      "error": "#B00020",
      "errorContainer": "#FADADD",
      "onErrorContainer": "#3B0008"
    },
  
  
    "radius": {
      "none": 0,
      "sm": 4,
      "md": 8,
      "lg": 14,
      "xl": 20
    },
  
  
    "padding": {
      "none": 0,
      "xs": 10,
      "sm": 16,
      "md": 24,
      "lg": 36,
      "xl": 44,
      "2xl": 56
    },


    "gap": {
      "xs": 10,
      "sm": 16,
      "md": 24,
      "lg": 36,
      "xl": 44,
      "2xl": 56
    },


    "textSize": {
      "xs": 17,
      "sm": 19,
      "md": 22,
      "lg": 28,
      "xl": 36
    },


    "textWeight": {
      "regular": 400,
      "medium": 600,
      "bold": 700
    },


    "lineHeight": {
      "tight": 1.35,
      "normal": 1.65,
      "relaxed": 1.9
    },


    "letterSpacing": {
      "tight": 0,
      "normal": 0.35,
      "loose": 0.8
    },
  
  
    "shadow": {
      "none": "none",
      "sm": "0px 1px 2px rgba(0,0,0,0.15)",
      "md": "0px 2px 4px rgba(0,0,0,0.2)",
      "lg": "0px 4px 8px rgba(0,0,0,0.25)"
    },
  
  
    "opacity": {
      "full": 1.0,
      "dim": 0.97,
      "low": 0.9
    },
  
  
    "borderWidth": {
      "none": 0,
      "sm": 2,
      "md": 3
    },
  
  
    "size": {
      "xs": 32,
      "sm": 48,
      "md": 64,
      "lg": 88
    },

    "textRole": {
      "display": { "size": "textSize.xl", "weight": "textWeight.bold", "lineHeight": "lineHeight.tight" },
      "headline": { "size": "textSize.lg", "weight": "textWeight.bold", "lineHeight": "lineHeight.tight" },
      "title": { "size": "textSize.md", "weight": "textWeight.medium", "lineHeight": "lineHeight.normal" },
      "body": { "size": "textSize.md", "weight": "textWeight.regular", "lineHeight": "lineHeight.normal" },
      "label": { "size": "textSize.sm", "weight": "textWeight.medium", "lineHeight": "lineHeight.normal" },
      "caption": { "size": "textSize.xs", "weight": "textWeight.regular", "lineHeight": "lineHeight.normal" }
    },

    "surfaceTier": {
      "base": { "background": "color.surface", "shadow": "shadow.none" },
      "raised": { "background": "color.surface", "shadow": "shadow.sm" },
      "overlay": { "background": "color.surface", "shadow": "shadow.md" },
      "floating": { "background": "color.surface", "shadow": "shadow.lg" }
    },

    "elevation": {
      "0": "none",
      "1": "0px 1px 2px rgba(0,0,0,0.15)",
      "2": "0px 2px 4px rgba(0,0,0,0.2)",
      "3": "0px 4px 8px rgba(0,0,0,0.25)",
      "4": "0px 8px 16px rgba(0,0,0,0.3)"
    },

    "prominence": {
      "primary": { "background": "color.primary", "color": "color.onPrimary" },
      "secondary": { "background": "color.surfaceVariant", "color": "color.onSurface" },
      "tertiary": { "background": "color.surface", "color": "color.onSurface" }
    },

    "fontFamily": {
      "base": "Lato, 'Open Sans', system-ui, sans-serif",
      "sans": "Lato, 'Open Sans', system-ui, sans-serif",
      "heading": "Lato, 'Open Sans', system-ui, sans-serif",
      "mono": "'Source Code Pro', ui-monospace, monospace"
    },

    "transition": {
      "fast": "150ms ease",
      "base": "200ms ease",
      "slow": "300ms ease",
      "none": "none"
    },

    "transform": {
      "hoverLift": "translateY(-1px)"
    }
  }
  
  
  