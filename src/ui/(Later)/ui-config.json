  "typography": {
    "fonts": {
      "default": "system",
      "sans": "system",
      "serif": "system",
      "mono": "system"
    },
    "scales": {
      "normal": {
        "xs": 11,
        "sm": 13,
        "base": 15,
        "lg": 17,
        "xl": 20,
        "2xl": 24,
        "3xl": 30
      },
      "large": {
        "xs": 12,
        "sm": 14,
        "base": 16,
        "lg": 19,
        "xl": 22,
        "2xl": 26,
        "3xl": 32
      }
    },
    "weights": {
      "light": 300,
      "normal": 400,
      "medium": 500,
      "semibold": 600,
      "bold": 700
    },
    "lineHeights": {
      "tight": 1.1,
      "normal": 1.4,
      "relaxed": 1.6
    }
  },

  "radius": {
    "none": 0,
    "xs": 2,
    "sm": 4,
    "md": 8,
    "lg": 12,
    "xl": 20,
    "full": 9999
  },

  "density": {
    "compact": 0.8,
    "comfortable": 1.0,
    "spacious": 1.2
  },

  "spacing": {
    "xs": 4,
    "sm": 8,
    "md": 12,
    "lg": 16,
    "xl": 24,
    "2xl": 32
  },

  "shadows": {
    "none": 0,
    "soft": 1,
    "medium": 2,
    "strong": 3
  },

  "borders": {
    "none": {
      "width": 0,
      "style": "solid"
    },
    "subtle": {
      "width": 1,
      "style": "solid"
    },
    "strong": {
      "width": 2,
      "style": "solid"
    }
  },

  "components": {
    "button": {
      "variants": {
        "primary": {
          "bg": "primary",
          "fg": "textOnPrimary",
          "borderColor": "primaryStrong",
          "shadow": "soft",
          "radius": "md",
          "weight": "medium"
        },
        "secondary": {
          "bg": "surfaceAlt",
          "fg": "textPrimary",
          "borderColor": "borderSubtle",
          "shadow": "none",
          "radius": "md",
          "weight": "medium"
        },
        "ghost": {
          "bg": "transparent",
          "fg": "primaryStrong",
          "borderColor": "transparent",
          "shadow": "none",
          "radius": "md",
          "weight": "medium"
        },
        "danger": {
          "bg": "danger",
          "fg": "textOnDanger",
          "borderColor": "dangerStrong",
          "shadow": "soft",
          "radius": "md",
          "weight": "medium"
        }
      },
      "sizes": {
        "sm": {
          "fontSize": "sm",
          "px": "sm",
          "py": "xs"
        },
        "md": {
          "fontSize": "base",
          "px": "md",
          "py": "sm"
        },
        "lg": {
          "fontSize": "lg",
          "px": "lg",
          "py": "md"
        }
      }
    },

    "card": {
      "variants": {
        "plain": {
          "bg": "surface",
          "fg": "textPrimary",
          "borderColor": "borderSubtle",
          "shadow": "none",
          "radius": "lg"
        },
        "elevated": {
          "bg": "surface",
          "fg": "textPrimary",
          "borderColor": "borderSubtle",
          "shadow": "soft",
          "radius": "lg"
        },
        "accent": {
          "bg": "primarySoft",
          "fg": "textPrimary",
          "borderColor": "primaryStrong",
          "shadow": "soft",
          "radius": "lg"
        }
      },
      "padding": {
        "sm": "sm",
        "md": "md",
        "lg": "lg"
      }
    },

    "input": {
      "variants": {
        "default": {
          "bg": "surface",
          "fg": "textPrimary",
          "borderColor": "borderSubtle",
          "radius": "md",
          "shadow": "none"
        },
        "subtle": {
          "bg": "surfaceAlt",
          "fg": "textPrimary",
          "borderColor": "borderSubtle",
          "radius": "md",
          "shadow": "none"
        },
        "underline": {
          "bg": "transparent",
          "fg": "textPrimary",
          "borderColor": "borderStrong",
          "radius": "none",
          "shadow": "none"
        }
      },
      "sizes": {
        "sm": {
          "fontSize": "sm",
          "py": "xs",
          "px": "sm"
        },
        "md": {
          "fontSize": "base",
          "py": "sm",
          "px": "md"
        },
        "lg": {
          "fontSize": "lg",
          "py": "md",
          "px": "lg"
        }
      }
    },

    "surface": {
      "levels": {
        "base": {
          "bg": "background"
        },
        "sunken": {
          "bg": "surfaceAlt"
        },
        "raised": {
          "bg": "surface"
        }
      }
    },

    "tag": {
      "variants": {
        "default": {
          "bg": "surfaceAlt",
          "fg": "textSecondary",
          "borderColor": "borderSubtle",
          "radius": "full"
        },
        "primary": {
          "bg": "primarySoft",
          "fg": "primaryStrong",
          "borderColor": "primaryStrong",
          "radius": "full"
        },
        "success": {
          "bg": "accentSoft",
          "fg": "accentStrong",
          "borderColor": "accentStrong",
          "radius": "full"
        },
        "danger": {
          "bg": "dangerSoft",
          "fg": "dangerStrong",
          "borderColor": "dangerStrong",
          "radius": "full"
        }
      },
      "sizes": {
        "sm": {
          "fontSize": "xs",
          "px": "sm",
          "py": "xs"
        },
        "md": {
          "fontSize": "sm",
          "px": "md",
          "py": "xs"
        }
      }
    }
  },

  "textVariants": {
    "display": {
      "fontSize": "3xl",
      "fontWeight": "bold",
      "lineHeight": "tight"
    },
    "headline": {
