      "format": "json",
      "schema": "any",
      "defaultValue": {}
    },
    "table": {
      "semantics": "Table",
      "format": "tabular",
      "schema": "rows+columns",
      "defaultValue": []
    },
    "profile": {
      "semantics": "UserProfile",
      "format": "object",
      "schema": "user-profile",
      "defaultValue": {}
    },
    "settings": {
      "semantics": "Settings",
      "format": "object",
      "schema": "settings",
      "defaultValue": {}
